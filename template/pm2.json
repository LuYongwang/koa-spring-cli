{
  "apps": [
    {
      "name": "koa-base-project",
      "script": "bin/server",
      "autorestart": true,
      "watch": false,
      "instances": 1,
      "max_memory_restart": "100M",
      "exec_interpreter": "babel-node",
      "exec_mode": "fork",
      "log_date_format": "YYYY - MM - DD HH: mm Z",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    }
  ]
}
